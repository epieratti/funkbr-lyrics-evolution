{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "funkbr processed_brcorpus record",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "accept_in_brcorpus": {"type": "integer"},
    "album_id": {"type": "string"},
    "album_name": {"type": "string"},
    "album_release_date": {"type": "string"},
    "album_release_precision": {"type": "string"},
    "album_total_tracks": {"type": "integer"},
    "album_type": {"type": "string"},
    "album_upc": {"type": "string"},
    "album_label": {"type": "string"},
    "artist_id": {"type": "string"},
    "artist_name": {"type": "string"},
    "artist_query": {"type": "string"},
    "available_in_BR": {"type": "boolean"},
    "available_markets": {
      "type": "array",
      "items": {"type": "string"}
    },
    "country_score": {"type": "integer"},
    "disc_number": {"type": "integer"},
    "duration_ms": {"type": "integer"},
    "explicit": {"type": "boolean"},
    "ingestion_ts": {"type": "integer"},
    "isrc": {"type": "string"},
    "isrc_score": {"type": "integer"},
    "market": {"type": "string"},
    "n_markets": {"type": "integer", "minimum": 0},
    "pt_hint": {"type": "integer"},
    "seed_match": {"type": "integer"},
    "track_id": {"type": "string"},
    "track_popularity": {"type": "integer", "minimum": 0, "maximum": 100},
    "track_name": {"type": "string"},
    "track_number": {"type": "integer"},
    "preview_url": {"type": ["string", "null"]},
    "ts": {"type": "integer"},
    "year_window_end": {"type": "integer"},
    "year_window_start": {"type": "integer"}
  },
  "required": [
    "artist_id",
    "artist_query",
    "track_id",
    "track_name",
    "market",
    "ts"
  ]
}
